package Local;



public class CalculatorProject {

	

	

	public int add(int a, int b) {

		

		return (a+b);

		

	}

	

	public int sub(int x, int y) {

		return (x-y);

		

	}

	

	public int mul(int p, int q) {

		return (p*q);

		

	}



	public int div(int s, int t) {

		

		return(s/t);

		

	}

	

	public double AreaOfCircle(int r) {

		return Math.PI*r*r;

	}

	

	public boolean postitiveNumber() {

		return true;

	}

}